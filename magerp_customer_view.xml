<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="view_partner_category_form_magerp" model="ir.ui.view">
			<field name="name">res.partner.category.form_magerp</field>
			<field name="model">res.partner.category</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="base.view_partner_category_form" />
			<field name="arch" type="xml">
				<field name="parent_id" position="after">
					<separator string="Magento Information" colspan="4" />
					<field name="magento_id" select="1" readonly="1" />
					<field name="tax_class_id" select="1" />
					<field name="instance" />
				</field>
			</field>
		</record>
		<record id="view_partner_address_form1_magerp1" model="ir.ui.view">
			<field name="name">res.partner.address.form1magerp1</field>
			<field name="model">res.partner.address</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="base.view_partner_address_form1" />
			<field name="arch" type="xml">
				<field name="name" position="after">
					<field name="lastname" />
				</field>
			</field>
		</record>
		<record id="view_partner_address_form1_magerp2" model="ir.ui.view">
			<field name="name">res.partner.address.form1magerp2</field>
			<field name="model">res.partner.address</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="base.view_partner_address_form1" />
			<field name="arch" type="xml">
				<field name="email" position="after">
					<separator string="Magento Specific Info" colspan="4" />
					<field name="entity_id" colspan="2" readonly="1" />
					<field name="instance" colspan="2" />
				</field>
			</field>
		</record>

	</data>
</openerp>